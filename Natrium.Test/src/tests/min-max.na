; min
min     r0  1   2
!assert  r0  1   
min     r0  -1 -2
!assert  r0  -2   
min     r0  0x1 0x2
!assert  r0  0x1 
mov     r1  1   
mov     r2  2   
min     r0  r1  r2
!assert  r0  1   

; max
max     r0  1   2
!assert  r0  2  
max     r0  -1  -2
!assert  r0  -1   
max     r0  0x1 0x2
!assert  r0  0x2 
mov     r1  1   
mov     r2  2   
max     r0  r1  r2
!assert  r0  2
