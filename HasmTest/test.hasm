@req:r4
@req:s2

# Test addition function.

main:
    # declare values
    mov r0 2
    mov r1 3
    
    # push values
    push r0
    push r1
    
    # call function
    jra f_add
    
    # pop and test result
    pop r0
    assert r0 5
    
    ret 

f_add:
    pop r3
    pop r2
    add r2 r2 r3
    push r2
    j ra # return to caller
